#: posts-from-images.php:32
#: posts-from-images.php:180
msgid "Posts from Images"
msgstr "Berichten maken van afbeeldingen"

#: posts-from-images.php:47
msgid "You do not have sufficient permissions to access this page."
msgstr "Je hebt onvoldoende rechten voor toegang van deze pagina."

#: posts-from-images.php:120
#, php-format
msgid "Failed to make a post for attachment '%1$s', because it is not an image. (ID: %2$s)"
msgstr "Kan geen bericht maken voor bijlage '%1$s', omdat het geen afbeelding is. (ID: %2$s)"

#: posts-from-images.php:147
#, php-format
msgid "Failed to make a post for attachment '%1$s'. (ID: %2$s)"
msgstr "Failed to make a post for attachment '%1$s'. (ID: %2$s)"

#: posts-from-images.php:153
#, php-format
msgid "Post made for attachment '%1$s'. (ID: %2$s, post ID: %3$s)"
msgstr "Bericht aangemaakt voor bijlage '%1$s'. (ID: %2$s, bericht ID: %3$s)"

#: posts-from-images.php:169
#, php-format
msgid "Finished in %s seconds"
msgstr "Afgerond in %s seconden"

#: posts-from-images.php:181
msgid "By"
msgstr "Door"

#: posts-from-images.php:188
msgid "Include these attachment IDs:"
msgstr "Sluit deze bijlage IDs in:"

#: posts-from-images.php:191
msgid "Enter comma-seperated attachment IDs to include for import."
msgstr "Vul door komma gescheiden bijlage IDs in om deze toe te voegen bij de import."

#: posts-from-images.php:196
msgid "Exclude these attachment IDs:"
msgstr "Sluit deze bijlage IDs uit:"

#: posts-from-images.php:199
msgid "Enter comma-seperated attachment IDs to exclude from import."
msgstr "Vul door komma gescheiden bijlage IDs in om deze toe te voegen bij de import."

#: posts-from-images.php:204
msgid "(Custom) Post Type:"
msgstr "(Aangepaste) Post Type"

#: posts-from-images.php:207
msgid "Make sure to enter it correctly, or just use 'post'."
msgstr "Maak er zeker van deze correct in te vullen, of gebruik gewoon 'post'."

#: posts-from-images.php:212
msgid "Set image as post thumbnail"
msgstr "Stel afbeelding in als bericht thumbnail"

#: posts-from-images.php:219
msgid "Title Format"
msgstr "Titel format"

#: posts-from-images.php:222
msgid "Use [title] for image title, [date] for image date, [ID] for attachment id and [i] for a sequential number."
msgstr "Gebruik [title] voor de titel van de afbeeldingen, [date] voor de datum van de afbeelding, [ID] voor het bijlage ID en [i] voor opeenvolgende nummers."

#: posts-from-images.php:223
msgid "Don't forget your seperators!"
msgstr "Vergeet je scheidingstekens niet!"

#: posts-from-images.php:228
msgid "Content Format"
msgstr "Content format"

#: posts-from-images.php:231
msgid "Use [url=*image_size*] for image url, [title] for image title, [date] for image date, [ID] for attachment id and [i] for a sequential number."
msgstr "Gebruik [url=*afbeeldingsgrootte*] voor de url van de afbeelding, [title] voor de titel van de afbeelding, [date] voor de datum van de afbeelding, [ID] voor het bijlage ID en [i] voor opeenvolgende nummers."

#: posts-from-images.php:232
msgid "Available image sizes:"
msgstr "Beschikbare afbeeldingsgroottes:"

#: posts-from-images.php:237
msgid "Date Format"
msgstr "Datum format"

#: posts-from-images.php:240
msgid "Use PHP date() formatting."
msgstr "Gebruik PHP date() formatting."

#: posts-from-images.php:245
msgid "Include [gallery] tag in post content?"
msgstr "Voeg [gallery] tag toe aan bericht inhoud?"

#: posts-from-images.php:253
msgid "Go!"
msgstr "Gaan met die banaan!"

